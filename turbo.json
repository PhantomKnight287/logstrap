{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist"
      ],
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "lint": {},
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "full"
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
