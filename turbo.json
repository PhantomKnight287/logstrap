{
    "$schema": "https://turbo.build/schema.json",
    "ui": "stream",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**",
                "out"
            ]
        },
        "test": {
            "cache": false
        },
        "lint": {},
        "types:check": {
            "dependsOn": [],
            "outputs": []
        },
        "clean": {
            "cache": false
        },
        "dev": {
            "cache": false
        }
    }
}